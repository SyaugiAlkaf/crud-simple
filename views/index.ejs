<!-- views/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CRUD App</title>
</head>
<body>
    <h1>Daftar Siswa</h1>
    <ul>
        <% data.forEach((siswa) => { %>
            <li><%= siswa.nama %> - <%= siswa.alamat %> - <%= siswa.hobi %></li>
        <% }); %>
    </ul>
</body>
</html>
